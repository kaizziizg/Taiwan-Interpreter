{
  "manifest_version": 2,

  "name": "Taiwan Interpreter",
  "description": "This extension use to translate word to taiwan local word",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "轉換中國詞彙吧!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
